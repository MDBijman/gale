{
    "name": "gale",
    "displayName": "gale",
    "description": "Gale syntax highlighting",
    "version": "0.0.1",
    "publisher": "mbijman",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gale",
