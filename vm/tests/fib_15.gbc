fn fib ($0: u64) -> u64
        movc $1, 2
        lt $1, $0, $1
        jmpifn $1 @lbl_1
        movc $1, 1
        ret $1
lbl_1:  movc $1, 1
        sub $2, $0, $1
        call $3, @fib ($2)
        movc $1, 2
        sub $2, $0, $1
        call $4, @fib ($2)
        add $1, $3, $4
        ret $1
endfn

fn main ($0: u64) -> u64
        movc $1, 15
        call $3, @fib ($1)
        ret $3
endfn