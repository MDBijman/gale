mod nested_calls

.const
.code
fn b ($0: ui64) -> ()
        movc $1, 1
        add $2, $0, $1
        ret $2
endfn

fn a ($0: ui64) -> ()
        call $1, @b ($0)
        ret $1
endfn

fn main ($0: &[&str]) -> ui64
        movc $1, 0
        idx $2, $0, $1, &str
        call $3, @std:parse_ui64 ($2)
        call $3, @a ($3)
        ret $3
endfn