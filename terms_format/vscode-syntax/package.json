{
    "name": "terms-syntax",
    "displayName": "terms-syntax",
    "description": "Syntax highlighting for .term files",
    "publisher": "mbijman",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "terms",
            "aliases": ["Terms", "terms"],
            "extensions": [".term"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "terms",
            "scopeName": "source.terms",
            "path": "./syntaxes/terms.tmLanguage.json"
        }]
    }
}