module short
import [std std.io]

fn main: () -> () = \() => {
	let a: std.ui64 = 1;
	let b: std.ui64 = 2;
	let is_true: std.ui64 -> std.bool = \n => { true };
	let side_effect: std.ui64 -> std.bool = \n => { std.io.println 3; true };
	if(is_true a || side_effect b) {
		std.io.print 1;
	} else {
		std.io.print 0;
	};
	()
};